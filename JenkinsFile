pipeline {
  agent {
      label 'agente diarias'
  }
  stages {
    stage('check') {
      steps {
        git(url: 'https://github.com/duduomena1/jenkins', branch: 'main')
      }
    }

    stage('list') {
      parallel {
        stage('list') {
          steps {
            sh 'ls -la'
          }
        }

    stage('Deploy') {
      steps {
        sh 'docker compose up -d'
      }
    }

  }
}
